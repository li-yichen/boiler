{

  "manifest_version": 2,
  "name": "Scroller",
  "version": "1.1",

  "description": "makes sound when scrolling",

  "web_accessible_resources": [
    "generator.mp3"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*","https://*.facebook.com/*","https://*.twitter.com/*"],
      "js": ["Scroller.js"]
    }
  ]

}